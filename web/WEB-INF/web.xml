<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
     <servlet>
        <servlet-name>admin_login</servlet-name>
        <servlet-class>SPCT.Admin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>admin_login</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>User_Reg</servlet-name>
        <servlet-class>SPCT.User_register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User_Reg</servlet-name>
        <url-pattern>/user_reg</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Getimage</servlet-name>
        <servlet-class>SPCT.Getimage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Getimage</servlet-name>
        <url-pattern>/Getimage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
